" Leader key
let mapleader = " "

source ~/.vim/init/options.vimrc            " settings

source ~/.vim/init/before_plug_loaded.vimrc " before plug loaded
source ~/.vim/init/plug.vimrc               " plugins
source ~/.vim/init/after_plug_loaded.vimrc  " after plug loaded

source ~/.vim/init/whichkey-mappings.vimrc  " which-key mappings
source ~/.vim/init/keybindings.vimrc        " key bindings

